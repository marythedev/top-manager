<div class="row">
    <div class="col-md-12">
        <h2>{{session.user.username}}</h2>
        <p>{{session.user.email}}</p>
        <hr />
        <div class="table-responsive">
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">Login History</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each session.user.history}}
                    <tr>
                        <th>{{dateTime}}</th>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>